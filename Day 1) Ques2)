
#include <stdio.h>

int main(){




    int a,b,sum,diff,product,quo;
    printf("enter the value of a : ");
    scanf("%d", &a);
    printf("enter the value of b : ");
    scanf("%d", &b);
    
    printf("sum %d\n", a+b);
    printf(" diff %d\n", a-b);
    printf("product %d\n", a/b);
    printf("quo %d\n", a/b);
    
    
    
    
    
    
    return 0;
}














   
   

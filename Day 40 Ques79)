#include <stdio.h>

int main() {
    int mat[10][10], rows, cols;

    printf("Enter number of rows: ");
    scanf("%d", &rows);

    printf("Enter number of columns: ");
    scanf("%d", &cols);

    // Input matrix
    printf("Enter the elements of the matrix:\n");
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            scanf("%d", &mat[i][j]);
        }
    }

    printf("\nDiagonal traversal of the matrix:\n");

    // Traverse diagonals
    for (int d = 0; d <= rows + cols - 2; d++) {
        for (int i = 0; i < rows; i++) {
            int j = d - i;
            if (j >= 0 && j < cols) {
                printf("%d ", mat[i][j]);
            }
        }
        printf("\n");
    }

    return 0;
}

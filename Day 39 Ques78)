#include <stdio.h>

int main() {
    int mat[10][10], n, sum = 0;

    printf("Enter the size of square matrix: ");
    scanf("%d", &n);

    // Input matrix
    printf("Enter the elements of the matrix:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &mat[i][j]);
        }
    }

    // Calculate sum of main diagonal
    for (int i = 0; i < n; i++) {
        sum += mat[i][i];
    }

    printf("Sum of main diagonal elements = %d\n", sum);

    return 0;
}

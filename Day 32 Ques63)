#include <stdio.h>

int main() {
    int n1, n2, arr1[100], arr2[100], merged[200];

    // Input for first array
    printf("Enter size of first array: ");
    scanf("%d", &n1);

    printf("Enter %d elements for first array: ", n1);
    for (int i = 0; i < n1; i++) {
        scanf("%d", &arr1[i]);
    }

    // Input for second array
    printf("Enter size of second array: ");
    scanf("%d", &n2);

    printf("Enter %d elements for second array: ", n2);
    for (int i = 0; i < n2; i++) {
        scanf("%d", &arr2[i]);
    }

    // Merge logic
    int k = 0;

    // Copy first array
    for (int i = 0; i < n1; i++) {
        merged[k++] = arr1[i];
    }

    // Copy second array
    for (int i = 0; i < n2; i++) {
        merged[k++] = arr2[i];
    }

    // Output merged array
    printf("Merged array: ");
    for (int i = 0; i < n1 + n2; i++) {
        printf("%d ", merged[i]);
    }

    return 0;
}
